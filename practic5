#include <stdio.h>

#define MOD 12345

// Функція для обчислення кількості послідовностей з заданою довжиною n
int countSequences(int n) {
    int dp[n + 1];  // Масив для зберігання проміжних результатів
    dp[0] = 1;  // Початкове значення
    dp[1] = 2;
    dp[2] = 4;

    // Заповнення масиву згідно рекурентної формули
    for (int i = 3; i <= n; i++) {
        dp[i] = (dp[i - 1] + dp[i - 2] + dp[i - 3]) % MOD;
    }

    return dp[n];  // Повертаємо кількість послідовностей
}

int main() {
    int n;
    printf("Введіть довжину послідовності n (1 < n < 10000): ");
    scanf("%d", &n);

    // Перевірка коректності введеного значення n
    while (n <= 1 || n >= 10000) {
        printf("Неправильне введення. Будь ласка, введіть число у діапазоні 1 < n < 10000: ");
        scanf("%d", &n);
    }

    int result = countSequences(n);  // Обчислення кількості послідовностей
    printf("Кількість шуканих послідовностей: %d\n", result);

}
